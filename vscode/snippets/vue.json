	"Vue Scaffold w/TypeScript": {
		"prefix": "vuets",
		"body": [
			"<template>",
			"\t$0",
			"</template>",
			"",
			"<script lang=\"ts\">",
			"import Vue from 'vue';",
			"import type { PropType } from 'vue';",
			"",
			"interface ComponentData {",
			"\t",
			"}",
			"",
			"export default Vue.extend({",
			"\tname: '${1:${TM_FILENAME_BASE}}',",
			"\tprops: {",
			"\t\titems: {",
			"\t\t\ttype: Array as PropType<string | string[]>",
			"\t\t\tdefault: []",
			"\t\t}",
			"\t},",
			"\tmethods: {",
			"\t\t",
			"\t},",
			"\tcomputed: {",
			"",
			"\t},",
			"\tdata(): ComponentData {",
			"\t\treturn {",
			"\t\t\t",
			"\t\t};",
			"\t}",
			"})",
			"</script>",
			"",
			"<style lang=\"scss\" scoped>",
			"",
			"</style>"
		],
		"description": "Scaffolds a new Vue component using TypeScript and SCSS."
	}
