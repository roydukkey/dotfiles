{
	"ignorePaths": [
		"dist",
		"node_modules/**/{.*/**/,.*/**/.,,.}*",
		".git/**",
		".jest/**",
		"**/.DS_Store",
		"*.lock.json",
		"*-lock.json",
		"*.lock"
	],
	"dictionaries": [
		"typescript"
	],
	// Before adding a word, see if there is already a dictionary in which it is contained.
	// http://github.com/streetsidesoftware/cspell-dicts
	"words": [
		"codereview", // codereview: Remove this word from configuration.
		"editorconfig",
		"roydukkey",
		"stroustrup",
		"tsdoc",
		"yarpm"
	]
}
